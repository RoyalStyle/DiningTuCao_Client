<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/simple_LinearLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TableLayout
        android:id="@+id/simple3_TableLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <TextView
       		android:id="@+id/simple3_textView1"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:textSize="20sp"
        	android:textStyle="bold"
        	android:text="" />
        <TextView
       		android:id="@+id/simple3_textView2"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:textSize="20sp"
        	android:text="" />
        <TableRow>
            <TextView android:id="@+id/simple3_textView3"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
    			android:textSize="20sp"
    			android:text="score:3"/>
			<TextView android:id="@+id/simple3_textView4"
    			android:layout_width="240dp"
    			android:layout_marginLeft="10dp"
    			android:layout_height="wrap_content"
    			android:textSize="15sp"
    			android:gravity="right"
    			android:text=""/>
		</TableRow>
    </TableLayout>

</LinearLayout>
